{
  "cSpell.words": [
    "dialector",
    "gonic",
    "gorm",
    "harshrastogi",
    "Infof",
    "Retailable",
    "simcompanies",
    "simcompaniesproxy",
    "sqlserver",
    "tailwindcss"
  ],

  "go.addTags": {
    "tags": "json",
    "options": "json=omitempty",
    "promptForTags": false,
    "transform": "snakecase",
    "template": ""
  },
  "gopls": {
    "ui.semanticTokens": true
  },

  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": ["document"],
    "cSpell.fixSpellingWithRenameProvider": false
  }
}
